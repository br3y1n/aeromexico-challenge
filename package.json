{
  "name": "aeromexico-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "launch": "ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/launch",
    "launch:all": "yarn launch --withCharacterTarget=api --withFavoriteTarget=api",
    "create:json-api": "ts-node scripts/create-json-api",
    "start:json-api": "yarn create:json-api && json-server json-db.json --port 8000",
    "sync:envs": "ts-node --project scripts/tsconfig.json -r tsconfig-paths/register scripts/sync-envs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./src ./__stories__ ./__tests__",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:fix": "eslint --fix ./src ./__stories__ ./__tests__ ./.storybook",
    "prettier": "prettier \"**/*.+(js|json|ts|tsx)\"",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "test": "vitest run",
    "validate": "yarn lint && yarn check-types && yarn test",
    "prepare": "husky"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.7",
    "next": "15.4.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-vitest": "^9.0.18",
    "@storybook/nextjs-vite": "^9.0.18",
    "@tailwindcss/postcss": "^4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "commander": "^14.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-storybook": "^9.0.18",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "storybook": "^9.0.18",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "author": "Brayan Arango <br3y1n@hotmail.es>",
  "license": "MIT"
}
